{
  "name": "vue-matchanalyzer",
  "version": "1.0.0",
  "description": "VueJS client to use MatchAnalyzer software",
  "main": "index.js",
  "scripts": {
    "start": "parcel -p 9999 ./src/index.html",
    "build": "parcel build ./src/index.html --no-minify",
    "docker": "cross-env DOCKER=true parcel build ./src/index.html --no-minify",
    "raspy": "cross-env RASPY=true parcel build ./src/index.html --no-minify",
    "lint": "eslint ./src --fix --ext .js,.vue",
    "prettier": "prettier ./src --check"
  },
  "keywords": [
    "node",
    "vue"
  ],
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "author": "manelmendez",
  "license": "MIT",
  "dependencies": {
    "@mdi/font": "latest",
    "animate.css": "latest",
    "axios": "latest",
    "chart.js": "^2.9.4",
    "font-awesome": "latest",
    "moment": "latest",
    "vue": "2.7.14",
    "vue-chartjs": "3.5.0",
    "vue-hot-reload-api": "latest",
    "vue-moment": "latest",
    "vue-router": "3.6.2",
    "vuetify": "latest",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/runtime": "latest",
    "@vue/component-compiler-utils": "latest",
    "babel-eslint": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-vue": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "parcel-bundler": "latest",
    "prettier": "latest",
    "sass": "latest",
    "vue-template-compiler": "latest"
  }
}
