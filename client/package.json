{
  "name": "vue-matchanalyzer",
  "version": "1.0.0",
  "description": "VueJS client to use MatchAnalyzer software",
  "main": "index.js",
  "scripts": {
    "start": "parcel -p 9999 ./src/index.html",
    "build": "parcel build ./src/index.html --no-minify",
    "docker": "cross-env DOCKER=true parcel build ./src/index.html --no-minify",
    "raspy": "cross-env RASPY=true parcel build ./src/index.html --no-minify",
    "lint": "eslint ./src --fix",
    "prettier": "prettier ./src --check"
  },
  "keywords": [
    "node",
    "vue"
  ],
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "author": "manelmendez",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "latest",
    "@mdi/font": "latest",
    "animate.css": "latest",
    "axios": "latest",
    "chart.js": "latest",
    "font-awesome": "latest",
    "moment": "latest",
    "vue": "latest",
    "vue-carousel": "latest",
    "vue-chartjs": "latest",
    "vue-hot-reload-api": "latest",
    "vue-router": "latest",
    "vuetify": "latest",
    "vuex": "latest"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@vue/component-compiler-utils": "latest",
    "cross-env": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "parcel-bundler": "latest",
    "prettier": "latest",
    "sass": "^1.26.10",
    "vue-template-compiler": "latest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint"
    ]
  }
}
